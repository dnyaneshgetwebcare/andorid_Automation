<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iplug.automation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_iplug_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_iplug_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AllSchedual"
            android:label="@string/title_activity_all_schedual"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".AddSchedual" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".SignIn" />
        <activity
            android:name=".LauncherActivity"
            android:label="@string/title_activity_launcher" />
        <activity android:name=".UserDataActivity" />
        <activity
            android:name=".ProfileActivity"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>